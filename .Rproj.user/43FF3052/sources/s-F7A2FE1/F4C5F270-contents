\name{Risk}
\alias{Risk}
\title{Evaluate VaR and ES}
\usage{
Risk(data, time)
}
\arguments{
\item{data}{A dataframe object with column dailyReturnRate}

\item{time}{A number of time window.}
}
\value{
A data frame with calculated VaR and ES
}
\description{
Evaluate VaR and ES
}
\examples{
data("returnRateEUR")
Risk(returnRateEUR, 200)
}
\keyword{Risk}
