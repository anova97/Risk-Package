\name{plotRisk}
\alias{plotRisk}
\title{Create a graph of calculated values of VaR and ES}
\usage{
plotRisk(data, time)
}
\arguments{
\item{data}{A dataframe object with column dailyReturnRate}

\item{time}{A number of time window.}
}
\value{
graph of calculated values of VaR or ES
}
\description{
Create a graph of calculated values of VaR and ES
}
